{{#if @model.length}}
  <ul>
    {{#each @model as |vocab|}}
      <li><a href={{vocab.url}}>{{vocab.name}}</a><button
          type='button'
          {{on 'click' (fn this.deleteVocab vocab)}}
        >🗑️</button><button
          type='button'
          {{on 'click' (fn this.createAndRunDownloadJob vocab.url)}}
        >{{this.downloadBtnTxt}}</button>{{vocab.id}}</li>
    {{/each}}
  </ul>
{{else}}
  <p>No vocabularies were found</p>
{{/if}}
<AddVocab />